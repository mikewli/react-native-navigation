{
  "name": "reactnativenavigation",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "install-android": "react-native run-android --variant=release",
    "release-android": "cd android && ./gradlew assembleRelease",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "podinstall": "cd ios && pod install",
    "clean": "watchman watch-del-all && rm -rf ios/DerivedData && rm -rf android/build && rm -rf android/app/build",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-navigation": "^7.28.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.2",
    "@types/react": "^18.0.12",
    "@types/react-native": "^0.67.8",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.7.3"
  },
  "jest": {
    "preset": "react-native"
  }
}